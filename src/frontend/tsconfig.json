{
  "compilerOptions": {
    "outDir": "../../public/js",
    "rootDir": ".",
    "module": "None",
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "strict": true,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "removeComments": false,
    "sourceMap": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}